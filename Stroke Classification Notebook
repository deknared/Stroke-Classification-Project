        "def prediction(data):\n",
        "    data_scaled = sc.transform(ct.transform(data))\n",
        "    predict = xgb.predict_proba(data_scaled)[:,1]\n",
        "    if predict > 0.05:\n",
        "        print(\"The person is likely to have a stroke.\")\n",
        "    else:\n",
        "        print(\"The person is not likely have a stroke.\")"
